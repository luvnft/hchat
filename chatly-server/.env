# 🌟 Server Configuration
# 🏷️ Name of the server
SERVER_NAME="Chatly"

# 📝 Description of the server
SERVER_DESCRIPTION="Welcome to Chatly!"

# 🚪 Port to run the API on
API_PORT=3000
# 🚪 Main websocket / api port for LiveKit
WS_PORT=7880
# 🚪 Used when the client could not connect via UDP (e.g. VPN, corporate firewalls)
ICE_TCP=7881
# 🚪 It's possible to handle all UDP traffic on a single port. When this is set - LiveKit Docs
ICE_UDP=7882

# 🔐 Live Kit SDK Configuration
# 🔑 API key (to be confirmed)
API_KEY="mykey"

# 🤫 API secret (to be confirmed)
API_SECRET="mysecret"

# 💎 Subscription Settings
# 🔛 Enable subscriptions for users
SUBSCRIPTION_ENABLED=false

# 📁 Max file size for subscription users
SUBSCRIPTION_MAX_FILE_SIZE_MB=100

# 📏 Max characters for subscription users
SUBSCRIPTION_MAX_CHARACTERS=1000

# 🎥 Max video quality for subscription users
SUBSCRIPTION_MAX_VIDEO_QUALITY_P=1080

# 🎙️ Max voice quality for subscription users
SUBSCRIPTION_MAX_VOICE_QUALITY_HZ=96000

# 📊 General User Limits
# 📂 Max file size for regular users
MAX_FILE_SIZE_MB=25

# ✍️ Max characters for regular users
MAX_CHARACTERS=500

# 📺 Max video quality for regular users
MAX_RESOLUTION_VIDEO_QUALITY_P=720

# 🔊 Max voice quality for regular users
MAX_VOICE_QUALITY_HZ=48000

# 🔑 License Configuration
# 🎫 License key for paid features
LICENSE_KEY="test"

# 🛠️ Development Settings
# 🐞 Enables development mode (affects CORS)
DEV_MODE=true
